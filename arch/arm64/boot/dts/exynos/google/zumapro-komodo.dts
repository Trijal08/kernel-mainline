/dts-v1/;

#include "zumapro-pixel-common.dtsi"

/ {
	model = "Pixel 9 Pro XL";
	compatible = "google,komodo", "google,zumapro-komodo", "google,zumapro";
};

&cont_splash_mem {
	// reg = <0x0 0xfac00000 (1440 * 3120 * 4)>;
	reg = <0x0 0x9c000000 (1344 * 2992 * 4)>;
	status = "okay";
};

&framebuffer0 {
	// width = <1440>;
	// height = <3120>;
	// stride = <(1440 * 4)>;
	width = <1344>;
	height = <2992>;
	stride = <(1344 * 4)>;
	// format = "a8r8g8b8";
	format = "x8r8g8b8";
	status = "okay";
};
